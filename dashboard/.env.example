# Environment variables for the dashboard
# Copy this file to .env.local and modify as needed

# Backend API URL (used by shared API hooks)
VITE_API_BASE_URL=http://localhost:8082

# WebSocket URL for real-time updates
VITE_WS_URL=ws://localhost:8082

# Development settings
VITE_DEV_MODE=true

# Production settings (uncomment for production builds)
# VITE_API_URL=https://your-production-api.com
# VITE_WS_URL=wss://your-production-api.com
# VITE_DEV_MODE=false